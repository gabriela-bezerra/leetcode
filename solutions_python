def removeDuplicates(nums):
    """ Remove duplicates from sorted array in place. """

    """
    Input: nums = [0,0,1,1,1,2,2,3,3,4]
    Output: 5, nums = [0,1,2,3,4,_,_,_,_,_]

    """

    # Two pointers approach

    l = 0
    r = 0

    for r in range(len(nums)):
        if nums[r] != nums[l]:
            l += 1
            nums[l] = nums[r]
    print(nums)
    return l + 1
